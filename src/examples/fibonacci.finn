import core::io;

// simple fibonacci example
func fibonacci(number: i32): i32 {
    if number <= 0 
        return 0;
    else if number <= 1
        return 1;
    else
        return fibonacci(number - 1) + fibonacci(number - 2);
}

func main(): i32 {
    io::println(fibonacci(10))!;
    return 0;
}